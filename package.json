{
  "name": "comtravo",
  "version": "1.0.0",
  "description": "A project or coding challenge for comtravo design and implemented by Abdulraheem Sherif A",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc --project ./",
    "prestart": "npm run build",
    "start": "nodemon src/app.ts",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config ./jest-cov.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./src/test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./apps/service/test/jest-e2e.json --detectOpenHandles --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adavuruku/comtravo.git"
  },
  "keywords": [
    "comtravo",
    "flight",
    "coding",
    "challenge"
  ],
  "author": "Abdulraheem Sherif Adavuruku",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adavuruku/comtravo/issues"
  },
  "homepage": "https://github.com/adavuruku/comtravo#readme",
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@types/node-cron": "^3.0.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^0.25.0",
    "babel-core": "^6.26.3",
    "concurrently": "^7.0.0",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "morgan": "^1.10.0",
    "node-cron": "3.0.0",
    "swagger-ui-express": "^4.3.0",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/config": "0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^27.4.6",
    "config": "^3.3.7",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.4.0",
    "typescript": "4.3.5"
  }
}
